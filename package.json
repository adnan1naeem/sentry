{
  "name": "sentry",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "release":"react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "androidSourceMap": "react-native bundle   --dev false   --platform android   --entry-file index.js   --bundle-output poc.bundle --sourcemap-output poc.bundle.android.map",
    "sentry": "node_modules/@sentry/cli/bin/sentry-cli  releases   -p 'react-native'  --org 'fatimaindustry'  files pocRelease upload-sourcemaps ./poc.bundle.android.map  ./poc.bundle  --rewrite --strip-prefix ./App.js",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@sentry/react-native": "^1.9.0",
    "react": "16.13.1",
    "react-native": "0.63.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
